{
  "name": "meanwifi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "node ./bin/www",
    "show-node-procs": "ps -ef | grep \"node \" | grep -v \"grep\"",
    "dev": "nodemon --exec \"npm run lint && node ./bin/www\"",
    "db-start": "sudo systemctl start mongod && sudo systemctl status mongod",
    "db-stop": "sudo systemctl stop mongod && sudo systemctl status mongod",
    "db-status": "sudo systemctl status mongod",
    "prod-restart": "heroku restart -a meanwifi",
    "prod-log-tail": "heroku logs -tail -a meanwifi"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.14.0"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "express": "4.16.1",
    "hbs": "4.1.1",
    "http-errors": "1.6.3",
    "mongoose": "5.9.24",
    "morgan": "1.9.1",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "nodemon": "2.0.4"
  }
}
