{
  "name": "meanwifi",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "DEBUG=meanwifi:tests mocha",
    "types": "tsc",
    "lint": "eslint .",
    "start": "node server.js",
    "debug": "DEBUG=meanwifi:* nodemon",
    "kill-node": "ps -ef | grep \"node \" | grep -v \"grep\" | awk '{print $2}' | xargs kill",
    "prod-restart": "heroku restart -a meanwifi",
    "prod-log-tail": "heroku logs -tail -a meanwifi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrnewton/meanwifi.git"
  },
  "author": "Jon Newton",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/jrnewton/meanwifi/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.0"
  },
  "dependencies": {
    "axios": "0.21.0",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "express": "4.16.1",
    "hbs": "4.1.1",
    "http-errors": "1.6.3",
    "luxon": "1.25.0",
    "mongoose": "5.9.24",
    "morgan": "1.9.1",
    "prettier": "2.1.2",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@types/luxon": "1.25.0",
    "@types/mocha": "8.0.4",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.11.2",
    "@types/supertest": "2.0.10",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "mocha": "8.1.2",
    "nodemon": "2.0.4",
    "supertest": "4.0.2",
    "typescript": "4.0.3"
  }
}
